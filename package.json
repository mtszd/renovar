{
  "name": "renovar",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "thumbnails": "node --loader ts-node/esm src/scripts/generateThumbnails.ts",
    "typecheck": "tsc --noEmit",
    "astro:check": "astro check",
    "svelte:check": "svelte-check",
    "lint": "eslint \"src/**/*.{ts,js,astro}\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check:all": "npm run astro:check && npm run svelte:check && npm run typecheck && npm run lint && npm run build",
    "check:ci": "npm run astro:check && npm run svelte:check && npm run typecheck && npm run lint && npm run format:check && npm run build"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.0.8",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/svelte": "^7.0.4",
    "@astrojs/vercel": "^8.0.5",
    "@vercel/node": "^2.3.0",
    "photoswipe": "^5.4.4",
    "resend": "^4.1.1",
    "svelte": "^5.19.6"
  },
  "devDependencies": {
    "@types/node": "^20.17.16",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "astro": "^5.2.3",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "sharp": "^0.33.5",
    "svelte-check": "^4.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.40.0"
  },
  "engines": {
    "node": ">=20.11 <21"
  }
}
